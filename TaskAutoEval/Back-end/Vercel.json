{
  "version": 2,
  "builds": [
    { "src": "api/**/*.go", "use": "@vercel/go" },
    { "src": "TAEForm.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/", "dest": "/TAEForm.html" },
    { "src": "/(.*)", "dest": "/TAEForm.html" }
  ],
  "functions": {
    "api/*.go": {
      "memory": 3008,
      "maxDuration": 30
    }
  },
  "env": {
    "MY_ENV_VAR": "value"
  }
}