{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.go",
        "use": "@vercel/go"
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/suggestions",
        "dest": "/api/suggestions"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  
